{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "feedland/feed-item-date",
  "parent": [
    "feedland/feed-query"
  ],
  "version": "0.1.0",
  "title": "Feed Item Date",
  "category": "widgets",
  "icon": "calendar-alt",
  "description": "Display the publish date for an entry.",
  "attributes": {
    "textAlign": {
      "type": "string"
    },
    "format": {
      "type": "string"
    },
    "isLink": {
      "type": "boolean",
      "default": false
    }
  },
  "supports": {
    "html": false,
    "color": {
      "gradients": true,
      "link": true,
      "__experimentalDefaultControls": {
        "background": true,
        "text": true,
        "link": true
      }
    },
    "spacing": {
      "margin": true,
      "padding": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "__experimentalFontFamily": true,
      "__experimentalFontWeight": true,
      "__experimentalFontStyle": true,
      "__experimentalTextTransform": true,
      "__experimentalTextDecoration": true,
      "__experimentalLetterSpacing": true,
      "__experimentalDefaultControls": {
        "fontSize": true
      }
    }
  },
  "usesContext": [
    "feedland/feed-item"
  ],
  "textdomain": "feedland",
  "editorScript": "file:./index.js",
  "render": "file:./render.php"
}